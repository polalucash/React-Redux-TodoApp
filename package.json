{
  "name": "react-redux-todo-app",
  "version": "0.1.0",
  "homepage": "https://polalucash.github.io/React-Redux-TodoApp",
  "private": true,
  "dependencies": {
    "husky": "^0.14.3",
    "json-server": "^0.12.0",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.71.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
